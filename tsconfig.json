{
  "compilerOptions": {
    "target": "ESNext",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": ["esnext", "dom"]
  },
  "vueCompilerOptions": {
    "nativeTags": ["block", "template", "component", "slot"]
  },
  "include": [
    "src/**/*.ts",
    "shims-uni.d.ts",
    "global.d.ts",
    "auto-imports.d.ts",
    "page.config.ts",
    "vite.config.ts",
    "uno.config.ts",
    "uni-pages.d.ts",
    "src/**/*.vue"
  ]
}
